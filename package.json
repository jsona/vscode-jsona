{
  "name": "vscode-jsona",
  "displayName": "JSONA syntax",
  "description": "Adds syntax highlighting of JSONA files",
  "version": "0.1.4",
  "publisher": "sigoden",
  "license": "MIT",
  "engines": {
    "vscode": "^1.3.0"
  },
  "icon": "icon.png",
  "homepage": "https://github.com/jsona/vscode-jsona-syntax",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsona/vscode-jsona-syntax"
  },
  "scripts": {
    "pkg": "vsce package"
  },
  "keywords": [
    "jsona",
    "syntax",
    "highlight"
  ],
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "jsona",
      "aliases": ["JSONA", "jsona"],
      "extensions": [".jsona"],
      "configuration": "./jsona.configuration.json"
    }],
    "grammars": [{
      "language": "jsona",
      "scopeName": "source.jsona",
      "path": "./syntaxes/jsona.json"
    }]
  }
}
